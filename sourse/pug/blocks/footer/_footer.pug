mixin footer()
	+b.FOOTER.footer.section
		.container
			+e.top
				+e.item
					+e.title Организатор
					+e.logo-wrap(href="#")
						+img('img/svg/logo-varmsu.svg')
				+e.item
					+e.title Контакты
					+e.mail(href="mailto:support@spremia.ru") support@spremia.ru
			+e.bottom 
				+e.item
					+e.title Премия Служение 2023 ©
				+e.item
					+e.title(href="#") Политика конфиденциальности
					+e.title(href="#") Публичная оферта