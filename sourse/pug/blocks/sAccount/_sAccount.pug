					//- 			a.sAccount__project-menu(href="#")
					//- 				span
					//- 			a.sAccount__project-btn.btn.btn-gold(href="#") Голосовать
					+projectItem('Название проекта 1 может быть разного размера')
					+projectItem('Название проекта 2')
					+projectItem('Название проекта 3')
					+projectItem('Название проекта 4')
